<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.junior">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ClickActivity" android:exported="true"></activity>
        <activity
            android:name=".BbsActivity"
            android:exported="true" />
        <activity
            android:name=".MarqueeActivity"
            android:exported="true" />
        <activity
            android:name=".ScrollActivity"
            android:exported="true" />
        <activity
            android:name=".GravityActivity"
            android:exported="true" />
        <activity
            android:name=".MarginActivity"
            android:exported="true" />
        <activity
            android:name=".ScreenActivity"
            android:exported="true" />
        <activity
            android:name=".PxActivity"
            android:exported="true" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>